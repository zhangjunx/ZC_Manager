package com.xr.entity;

/**
 * @ClassName KQ_Approver
 * @Description 审批记录表
 * @Author csc
 * @Date 2019年10月18日 下午3:12:59
 */
public class KQ_Approver {

	 private Integer approverno;//主键编号
	 
	 private Integer themeno;//主题编号,用于使用关联主题表
	 
	 private String rankno;//几级审批人
	 
	 private String holderno;//员工工号
	 
	 private String auditstatus;//审批状态(12是同意,13是驳回)
	 
	 private String remark;//审批意见
	 
	 private String approvaltime;//审批时间

	 
	 //get和set方法
	public Integer getApproverno() {
		return approverno;
	}

	public void setApproverno(Integer approverno) {
		this.approverno = approverno;
	}

	public Integer getThemeno() {
		return themeno;
	}

	public void setThemeno(Integer themeno) {
		this.themeno = themeno;
	}

	public String getRankno() {
		return rankno;
	}

	public void setRankno(String rankno) {
		this.rankno = rankno;
	}

	public String getHolderno() {
		return holderno;
	}

	public void setHolderno(String holderno) {
		this.holderno = holderno;
	}

	public String getAuditstatus() {
		return auditstatus;
	}

	public void setAuditstatus(String auditstatus) {
		this.auditstatus = auditstatus;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getApprovaltime() {
		return approvaltime;
	}

	public void setApprovaltime(String approvaltime) {
		this.approvaltime = approvaltime;
	}
	 
	 
	
}
