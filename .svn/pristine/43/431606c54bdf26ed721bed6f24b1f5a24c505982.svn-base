}
.approval_self .approval_right .dept .deptTree::-webkit-scrollbar-thumb:hover{
  background: #999;
}
.approval_self .approval_right .dept .deptTree::-webkit-scrollbar-corner{
  background: #179a16;
}
.approval_self .approval_right .dept .dept_title{
	font-size:18px;
	color:#008EDE;
	padding-left:60px;
	background:url(../image/wx_approver3.png) no-repeat 20px center;
