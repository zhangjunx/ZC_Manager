.selectY{
	padding: 5px 10px 6px 5px;
    outline: none;
    background: none;
    width: 233px;
    color: #666;
    border: 1px solid #ddd;
    height: 32px;
    line-height: 32px;
    margin-right: 5px;
    border-radius: 3px;
    box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
}
.selectDoorList{
	width: 100%;
    height: calc(100% - 60px);
    overflow: auto;
    padding-top: 8px;
    padding-bottom: 15px;
    box-sizing: border-box;
}
.selectDoorList li{
	border:1px solid #ddd;
	display:inline-block;
	padding:10px;
	box-sizing:border-box;
	margin:5px 15px;
	border-radius:5px;
	cursor:pointer;
	width:200px;
	text-align:center;
}
.selectDoorList li:hover{
	opacity:0.8;
	transition:all 0.5s ease;
}
.selectDoorList li.current{
	border:1px solid #2277cc;
}
.userInput{
	width: 140px;
    height: 30px;
    line-height: 30px;
    border: 1px solid #ddd;
    color: #333;
    background: #fff;
    margin-right: 5px;
    border-radius: 3px;
    box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
    vertical-align: middle;
}
