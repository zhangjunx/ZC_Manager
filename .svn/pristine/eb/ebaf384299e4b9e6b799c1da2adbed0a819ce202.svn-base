#shuttle_box{
	width:1200px;
	zoom: 1;
	margin: 50px auto;  
	padding-left: 136px;
    box-sizing: border-box;
}}
#shuttle_box:after{
   content: ".";
   clear: both;
   display: block;
   height: 0;
   overflow: hidden;
   visibility: hidden;
}
.shuttle_box_li{
    height: 640px;
    float: left;
    
}
.shuttle_box_near .title{
    background: #F5F7FA;
    padding-left: 10px;
