<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>备品备件基本资料</title>
    <link rel="stylesheet" href="../css/common.css" />
    <link rel="stylesheet" href="../css/public1.css" />
    <link rel="stylesheet" href="../css/list_style.css" />
     <link rel="stylesheet" href="../layui/css/layui.css"> 
      <link rel="stylesheet" href="../zjx/purchase_list.css">
   
</head>

<body>
    <div id="cme-main">
        <div class="main-content">
            <div class="main-tab">
                <div class="label fl curr"><a href="javascript:;">备品件基本资料</a></div>
                <div class="btn-box fr">
                         <div class="subbtn fl"><a href="b_SparePartsData_add.html" class="blue addMain" id="insertSparePartsData">新增</a></div>
                    	 <div class="subbtn fl"><a href="javascript:;" class="blue delMain" id="deleteSparePartsData">删除</a></div>
                    	 <div class="subbtn fl"><a href="b_importFile.html" class="blue addMain" id="daoruSpareParts">批量导入</a></div>
                    <div class="subbtn fl"><a href="javascript:;" class="orange" onclick="printTable('dayindaju1')" id="printSparePartsData">打印</a></div>
                </div>
                <div class="main-table">
                    <!--查询条件-->
                    <div class="search-box main-form fl">
                        <div class="form-term fl clearfix">
                            <div class="lable"><span></span>备品件条码：</div>
                            <div class="control clearfix">
                                <input type="text" id="itemcode1" name="itemcode" placeholder="请输入物品条码" class="input"  />
                            </div>
                        </div>
                        <div class="form-term fl clearfix">
                            <div class="lable"><span></span>备品件名称：</div>
                            <div class="control clearfix">
                                <input type="text" id="itemname1" name="itemname" placeholder="请输入物品名称" class="input">
                            </div>
                        </div>
                        
                        <div class="form-term fl clearfix">
                            <div class="lable"><span></span>所属类别：</div>
                            <div class="control clearfix">
                               <select id="itemtype1">
                                     <option value="">--请选择--</option>
                               </select>
                            </div>
                        </div>
                        
                        <div class="btn fl">
                            <a href="javascript:;" id="queryBtn">搜索</a>
                        </div>
                    </div>
                    <!--查询结果如下-->
                    <table class="grade" id="dayindaju1">
                            <tr>
                            	<td width="40" class="no-print"><span class="checkbox" id="selectAll" style="float: none;">全选</span></td>
                            	<!-- <td width="100">序号</td> -->
                                <td width="100">备品件条码</td>
                                <td width="100">备品件名称</td>
                                <td width="100">规格</td>
                                <td width="100">型号</td>
                                <td width="100">计量单位</td>
                                <td width="100">所属类别</td>
                                <td width="100">备注说明</td>
                                <td width="100" class="no-print">操作</td>
                            </tr>
                             <tbody class="Splicing" id="cont">
                            </tbody>
                    </table>
                    <div class="pages" id="test"></div> 
                </div>
            </div>
        </div>
  </div>
    <script type="text/javascript" src="../hyf/path_js.js"></script>
    <script type="text/javascript" src="../js/jquery-3.3.1.min.js"></script>
    <script type="text/javascript" src="../js/layer/layer.js"></script>
    <script type="text/javascript" src="../js/common.js"></script>
    <script type="text/javascript" src="../layui/layui.all.js"></script>
     <!-- <script type="text/javascript" src="chu/b_SparePartsData_list.js"></script> -->
      <script type="text/javascript" src="js/b_SparePartsData_list.js"></script>
      <script type="text/javascript" src="../js/jquery-1.4.4.min.js"></script>
	 <script type="text/javascript" src="../js/jquery.jqprint-0.3.js"></script>
</body>
</html>
