/* 首页 */

html {
    height: 100%;
}

body {
    height: 100%;
}

.top {
    background: url(../img/bg.png) no-repeat;
    width: 100%;
    height: 100%;
    overflow: hidden;
    padding:0;
}
.top .copyright{
	height:105px;
	line-height:105px;
	text-align:center;
	font-size:42px;
	font-weight:normal;
	color:#fff;
	letter-spacing:2px;
	box-sizing:border-box;
	background:rgba(0,0,0,0.6);
}
.top .bottom_copyright{
	box-sizing:border-box;
}
.top .row_top{
	padding-bottom:10px;
	box-sizing:border-box;
	/* border-bottom:2px solid #003A77; */
	background:url(../img/bg_line.png) repeat-x left bottom;
}
.top .row_top .row_title{
	text-align:center;
	
}
.top .row_top .title_system{
	margin-top:20px;
}

.top .logo {
    margin-top: 20px;
    margin-left: 40px;
}

.top .time {
    text-align: right;
    padding-right: 40px;
    padding-top:13px;
    box-sizing: border-box;
}

.top .time .hour {
    color: #fff;
    font-size: 36PX;
    font-weight: 400;
    padding-right: 28px;
}

.top .time .date {
    color: #fff;
    font-size: 36px;
    font-weight: 400;
}

.top .con {
    height: calc(100% - 222px);
    display: flex;
    justify-content: center;
    align-items: center;
    
}

.menu {
    width: 1045px;
    overflow: hidden;
    margin: 0 auto;
}

.menu li {
    width: 482px;
    height: 313px;
    text-align: center;
    float: left;
    cursor: pointer;
}

.menu li:hover {
    opacity: 0.8;
    transition: all ease 0.5s;
}

.menu li.first {
    background: linear-gradient(-41deg, rgba(229, 92, 0, 1), rgba(250, 139, 42, 1));
    border-radius: 10px;
    margin-right: 80px;
}

.menu li.second {
    background: linear-gradient(-41deg, rgba(3, 87, 194, 1), rgba(0, 153, 227, 1));
    border-radius: 10px;
    margin-bottom: 50px;
}

.menu li.third {
    background: linear-gradient(-41deg, rgba(43, 126, 10, 1), rgba(71, 164, 24, 1));
    border-radius: 10px;
    margin-right: 80px;
}

.menu li.fourth {
    background: linear-gradient(-41deg, rgba(112, 32, 165, 1), rgba(162, 77, 208, 1));
    border-radius: 10px;
}

.menu li img {
    margin-top: 47px;
}

.menu li p {
    color: #fff;
    font-size: 48px;
}

.menu li p.person {
    padding-top: 40px;
    box-sizing: border-box;
}

.menu li p.vehicle {
    padding-top: 26px;
    box-sizing: border-box;
}

.menu li p.audited {
    padding-top: 40px;
    box-sizing: border-box;
}

.menu li p.unaudited {
    padding-top: 29px;
    box-sizing: border-box;
}


