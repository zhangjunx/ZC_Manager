//接口验证labelcode是否存在
function checkLabelRepeat(arr){
	var flags=true;
	$.ajax({
		url:url+"/zcInStore/checkLabelRepeat",
		type:"post",
		data:JSON.stringify({"goodsList":arr}),
		async:false,
		contentType:"application/json",
		success:function(data){
			if(data.flag){
				flags=true;
			}else{
				layer.msg(data.reason,{time:2000});
				flags=false;
			}
		}
	})
	return flags;
}
