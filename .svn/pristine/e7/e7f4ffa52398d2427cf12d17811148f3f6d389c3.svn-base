	background: #fff;
	margin-right: 5px;
	border-radius: 3px;
	box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
}

/*input结束*/
/*select开始*/
.main-form .form-term .control select {
	/*float:left;*/
	padding: 5px 10px 6px 5px;
	outline: none;
	background: none;
	width: 233px;
	color: #666;
	border: 1px solid #ddd;
	height: 32px;
	line-height: 32px;
	overflow: hidden;
	margin-right: 5px;
	border-radius: 3px;
	box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
}

/*select结束*/
/*上传图片开始*/
