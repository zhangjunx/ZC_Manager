@charset "utf-8";



.pages {
	margin-top: 25px;
	text-align: center;
	overflow: hidden;
	zoom: 1;
}

.pages a,
.pages span {
	font-size: 12px;
	text-decoration: none;
	display: inline-block;
	height: 30px;
	line-height: 30px;
	margin-left: 4px;
	cursor: pointer;
	transition: all .2s;
	padding: 0 15px;
	border-radius: 2px;
	border: 1px solid #ddd;
	color: #666;
	background-color: #fff;
}

.pages .curr {
	border: 1px solid #3aace9;
	color: #fff;
	background-color: #3aace9;
}

.pages span:not(.current) {
	cursor: not-allowed;
}

.pages .input {
	width: 44px;
	display: inline-block;
	height: 30px;
	line-height: 30px;
	border-radius: 2px;
	border: 1px solid #ddd;
	color: #666;
	margin-left: 4px;
}

/*表格 开始*/
table {
	width: 100%;
	/*table-layout: fixed;*/
}

table tr td {
	text-align: center;
	border: 1px solid #ddd;
	padding: 4px;
	font-size: 12px;
	color: #333;
	word-break: keep-all;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

table tr:first-of-type td {
	background: #f2f2f2;
	font-weight: bold;
	text-align: center;
	height: 28px;
	line-height: 28px;
}

table tr:hover td {
	background: #f6fbff;
}

table tr:hover:first-of-type td {
	background: #f2f2f2;
}

table tr td a {
	display: inline-block;
	color: #4c9dfb;
	margin: 0 2px;
}

table tr td a:hover {
	text-decoration: underline;
	color: #4c9dfb;
}

table tr td input {
	max-width: -moz-calc(100% - 40px);
	max-width: -webkit-calc(100% - 40px);
	max-width: calc(100% - 40px);
	height: 26px;
	line-height: 26px;
	padding: 0 10px;
	border: 1px solid #ddd;
	outline: none;
}

table tr td select {
	max-width: -moz-calc(100% - 20px);
	max-width: -webkit-calc(100% - 20px);
	max-width: calc(100% - 20px);
	height: 28px;
	border: 1px solid #ddd;
	outline: none;
}

table tr td textarea {
	max-width: -moz-calc(100% - 40px);
	max-width: -webkit-calc(100% - 40px);
	max-width: calc(100% - 40px);
	height: 26px;
	line-height: 26px;
	padding: 0 10px;
	border: 1px solid #ddd;
	outline: none;
	color: #333;
}

