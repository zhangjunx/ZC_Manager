/*main表单   开始*/
.main-form {
	margin: 15px 0;
}

.main-form .form-term {
	display: block;
	min-height: 30px;
	clear: both;
	margin-bottom: 8px;
}

.main-form .form-term .lable {
	float: left;
	width: 116px;
	height: 28px;
	line-height: 16px;
	text-align: right;
	font-size: 14px;
	color: #666;
	margin-right: 10px;
	margin-top: 6px;
}

.main-form .form-term .lable span {
	width: 10px;
	display: inline-block;
	color: #f04d4a;
	font-weight: 700;
	padding-right: 5px;
}

.main-form .form-term .control {
	float: left;
	width: -moz-calc(100% - 126px);
	width: -webkit-calc(100% - 126px);
	width: calc(100% - 126px);
}

/*input开始*/
.main-form .form-term .control .input {
	width: 200px;
	height: 30px;
	line-height: 30px;
	border: 1px solid #ddd;
