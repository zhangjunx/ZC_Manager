.main-process{}
.main-process .sub-process{ height: 110px; margin-bottom: 16px;}
.main-process .sub-process .main-process-left{ width: 100px; padding:20px 26px 0 0; text-align: right; font-size: 14px; color: #666;font-weight: 600;}
.main-process .sub-process .main-process-left span{ display: block; line-height: 26px;}
.main-process .sub-process .main-process-right{position: relative; width: calc(100% - 130px); width: -moz-calc(100% 130px); width: -webkit-calc(100% - 130px);}
