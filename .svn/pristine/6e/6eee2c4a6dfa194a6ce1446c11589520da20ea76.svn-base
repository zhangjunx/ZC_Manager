}

#myProgress{
	background: #fff;
    width: 50%;
    height: 20px;
    position: absolute;
    top: 50%;
    left: 25%;
    border-radius: 5px;
    display:none;
}
#myProgress a{
	color:#fff;
}

#myProgress #myBar{
	width: 0%;
	height: 100%;
	background-color: #4CAF50;
