/* 鎴戠殑鑰冨嫟琛� */
.mychart{
	clear: both;
    border: 1px solid #eee;
    padding-top: 20px;
    overflow: hidden;
}
.mychart_top{
	float:left;
	padding-left: 20px;
}
.mychart .mychart_top .mon{
	display:inline-block;
    border: 1px solid #eee;
    border-radius: 10px;
    margin-left:20px;
    margin-right:20px;
    font-size:14px;
    width: 49px;
    text-align: center;
    height: 41px;
    line-height: 41px;
}
.mychart .mychart_top .year{
	padding: 5px 10px 6px 5px;
    outline: none;
    background: none;
    width: 110px;
    color: #666;
    border: 1px solid #ddd;
    height: 41px;
    line-height: 32px;
    overflow: hidden;
    margin-right: 5px;
    border-radius: 3px;
    box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
    font-size:20px;
}
.mychart .form-term .lable{
	float: left;
    width: 116px;
    height: 28px;
    line-height: 16px;
    text-align: right;
    font-size: 14px;
    color: #666;
    margin-right: 10px;
    margin-top: 6px;
}
.mychart .form-term .lable span{
	color:#666;
}
.mychart table{
	margin-top:70px;
}
.mychart table tr th{
	border:1px solid #eee;
	line-height:40px;
}
.mychart table tr td{
	padding:10px 0;
}
.mychart table tr td span{
	color:#666;
}
.mychart table tr td i{
	font-size:30px;
	color:#ccc;
}
.mychart table tr td .status{
	padding:5px 10px;
	background:#eee;
	border-radius:15px;
	width:50%;
	margin:0 auto; 
}
.mychart table tr:hover td{
	background:#fff;
}
.mychart .mychart_top img{
	cursor:pointer;
}
.mychart .mychart_top img:hover{
	opacity:0.5;
	transition:all 0.5s ease;
}

