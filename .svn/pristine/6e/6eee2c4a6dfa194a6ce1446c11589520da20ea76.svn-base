}
.holder_add{
	position: absolute;
    left: 4px;
    top: 4px;
    right: 8px;
    bottom: 8px;
    background: #000;
    opacity: 0.5;
    display: none;  
}
.holder_box{
	width: 500px;
   /*  height: 180px; */
    background: #fff;
    position: absolute;
    left: 40%;
    top: 30%;
    display: none;  
    padding-bottom:10px;
}
.holder_box .layer_top{
	background: #5498E5;
    padding: 10px 10px;
    box-sizing: border-box;
}
.holder_box .layer_top h2{
	display: inline-block;
    color: #fff;
}
.holder_box .layer_top img{
	float: right;
    margin-top: -3px;
    cursor: pointer;
}
.holder_box .holder_top p{
	line-height:30px;
	padding-left:100px;
	box-sizing:border-box;
}
.holder_box .holder_top p span{
	margin-left:5px;
}
.holder_box .holder_bottom{
	margin-top:10px;
	display:flex;
	justify-content: space-around;
}
.holder_box .holder_bottom a{
	display: inline-block;
    padding: 0 16px;
    line-height: 28px;
    color: #fff;
    border-radius: 3px;
    margin-right:20px;
}
.holder_box .door_top{
	padding-left:20px;
	border-bottom:1px solid #eee;
	padding-top:10px;
	padding-bottom:20px;
	box-sizing:border-box;
	position:relative;
}
.holder_box .door_top .checkbox{
	font-size: 12px;
    color: #555;
    display: inline-block;
    height: 30px;
    line-height: 30px;
    margin-right: 8px;
    padding-left: 16px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    background: url(../images/icons/form_check.png) no-repeat left 6px;
    position: absolute;
    top: 0px;
    left: 20px;
}
.holder_box .door_top .cur{
	background: url(../images/icons/form_check_on.png) no-repeat right 25px top 9px;
}
.holder_box .door_center{
	height: 150px;
    padding-left: 20px;
    padding-top: 10px;
    overflow:auto;
}
.holder_box .door_center textarea{
	width: 456px;
    height: 120px;
    resize: none;
    border: 1px solid #ddd;
    padding: 5px 5px;
    box-sizing: border-box;
}
