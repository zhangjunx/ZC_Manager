.timeline-box .timeline-content{overflow: hidden;width:calc(100% - 60px);width: -moz-calc(100% - 60px);width: -webkit-calc(100% - 60px);height:110px;position: relative;}
.timeline-box .timeline-content .main-line{height:4px;background: #eee; margin-top:42px; border-radius:2px; position: relative;}
.main-process .sub-process .main-process-right .main-line .time-line{ position: absolute; top: 0; left: 5%; height: 4px; background: #52C41A; display: inline-block; border-radius:2px;}
.main-process .sub-process .main-process-right .main-line .time-lines{ position: absolute; top: 0; right: 5%; height: 4px; background: #52C41A; display: inline-block; border-radius:2px;}
.main-process .sub-process .main-process-right .main-title{
	position: relative;
    top: 15px;
    left: 108px;
}
.timeline-box .timeline-content .timeline-list{position: absolute; top:0;width:100%; min-width: 1100px; overflow-y: auto;}			
.timeline-box .timeline-content .timeline-list .list{width:8%;height:110px;position: relative;float:left; margin: 0 1%;}			
.timeline-box .timeline-content .timeline-list .list .time{ text-align: center; line-height: 30px; padding-top: 5px; color: #333;}
.timeline-box .timeline-content .timeline-list .list .node{position: absolute;left:50%;top:50%;margin:-19px 0 0 -9px;width: 16px;height: 16px;border-radius: 50%;background: #fff;border: 1px solid #ddd;}
.timeline-box .timeline-content .timeline-list .list .node.green{ border: 4px solid #30c430;width: 10px;height: 10px;}
.timeline-box .timeline-content .timeline-list .list .node.orange{ border: 4px solid #edc024;width: 10px;height: 10px;}
.timeline-box .timeline-content .timeline-list .list .node.red{ border: 4px solid #ed3535;width: 10px;height: 10px;}
.timeline-box .timeline-content .timeline-list .list .info{width:110px;position:absolute;top:50%;left:50%;margin:10px 0 0 -56px;box-sizing: border-box;border:1px solid #ddd;border-radius: 4px;background-color:#fff;}
.timeline-box .timeline-content .timeline-list .list .info:hover{box-shadow: 0 1px 4px rgba(34,25,25,0.4);}
.timeline-box .timeline-content .timeline-list .list .arrow{background: url(../images/icons/time_arrow.png) no-repeat;position: absolute;top:-11px;left:50%; margin-left:-8px;width:16px;height: 11px;}
.timeline-box .timeline-content .timeline-list .list .info .name{margin-left:4px; line-height: 22px; padding: 5px; text-align: center; color: #666;}
.timeline-box .timeline-content .timeline-list .list.up .arrow{background: url(images/icons/layer_arrow_up.png) no-repeat;position: absolute;top:100%;left:50%;margin-left:-8px;width:16px;height: 11px;}
.timeline-box .timeline-content .timeline-list .list.up .info{margin:-65px 0 0 -80px;box-sizing: border-box;}
.timeline-box .timeline-content .timeline-list .list.up .info:hover{box-shadow: 0 1px 4px rgba(34,25,25,0.4);}
