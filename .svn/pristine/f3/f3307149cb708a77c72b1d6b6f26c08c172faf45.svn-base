/* 小程序管理  权限分配页面   */
.appletPower .role_title{
	overflow:hidden;
	padding:10px 10px;
	box-sizing:border-box;
	border-bottom:1px solid #ddd;
	margin-bottom:10px;
}
.appletPower .role_title span.checkbox{
	height: 18px;
    line-height: 18px;
    float: left;
    display: inline-block;
    padding-left: 16px;
    width: auto;
    margin-right: 15px;
    overflow: hidden;
    background: url(../image/form_check.png) no-repeat left 0px;
    margin-top:6px;
    cursor:pointer;
    font-size:14px;
}
.appletPower .role_title span.curr{
	display: inline-block;
    padding-left: 16px;
    width: auto;
    margin-right: 15px;
    overflow: hidden;
    background: url(../image/form_check_on.png) no-repeat left 3px;
}
.appletPower .role_title a{
	float:right;
	margin-right:20px;
}
.appletPower .applet_role{
	overflow:hidden;
	height: calc(100% - 115px);
    overflow: auto;
}
.appletPower .role_add{
	padding-left:10px;
	box-sizing:border-box;
	margin-bottom:10px;
	display:none;
}
.appletPower .role_add img{
	cursor:pointer;
}
.appletPower .role_add img.sure{
	margin-right:10px;
}
.appletPower .role_add .ipt{
	width: 135px;
    height: 30px;
    line-height: 30px;
    border: 1px solid #ddd;
    color: #333;
    background: #fff;
    margin-right: 12px;
    border-radius: 3px;
    box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
}
.appletPower .applet_role li{
	padding: 10px 10px;
	box-sizing:border-box;
	cursor:pointer;
	margin:1px 0;
	font-size:14px;
}
.appletPower .applet_role li.current span.checkbox{
	background:#e9f2f6;
	display: inline-block;
    padding-left: 16px;
    width: auto;
    margin-right: 15px;
    overflow: hidden;
    background: url(../image/form_check_on.png) no-repeat left 3px;
}
.appletPower .applet_role li.current{
	background:#e9f2f6;
}
.appletPower .applet_role li:hover{
	background:#e9f2f6;
}
.appletPower .applet_role li span.checkbox{
	height: 18px;
    line-height: 18px;
    float: left;
    display: inline-block;
    padding-left: 16px;
    width: auto;
    margin-right: 15px;
    overflow: hidden;
    background: url(../image/form_check.png) no-repeat left 0px;
    cursor:pointer;
}
/* 小程序管理上传图片页面 */
.img_box{
	position:relative;
}
.img_box .img{
	width:200px;
	height:150px;
}
.img_box .uploadImage{
	width:200px;
	height:150px;
	opacity:0;
	position:absolute;
	left:0;
	top:0;
}
.applet_box{
	padding: 10px;
    box-sizing: border-box;
    height: calc(100% - 61px);
    overflow: auto;
    width:100%;
}
.applet_box .space_box{
	margin:10px;	
}
.applet_box .addBox{
	margin:10px;
}
.applet_box .space_box .image_text{
	text-align:center;
	margin-top:15px;
}
.applet_box .add_Img{
	width: 200px;
    height: 148px;
    border: 1px solid #ddd;
    text-align: center;
    font-size: 79px;
    line-height: 130px;
    color: #ddd;
    cursor:pointer;
}
.applet_box .add_Img:hover{
	color:#bbb;
	border:1px solid #bbb;
	transition:all 0.5s ease;
}