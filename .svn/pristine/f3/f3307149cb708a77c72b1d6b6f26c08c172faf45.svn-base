/* 权限分配 */
#cme-main .main-content{
	width:100%;
	height:100%;
	/* overflow:auto; */
}
#cme-main .main-content .main-tab{
	width:98%;
}

.rolepermission{
	width:100%;
}

.main-form{
	margin: 20px auto;
    width: 95%;
    height: 90%;
    border-radius: 10px;
    border: 1px solid #ddd;
}

.main-form .label{
	 border-top-left-radius: 10px;
	padding:10px 10px;
	border-bottom:1px solid #ddd;
	font-size:18px;
	color:#666;
	background:#F5F7FA;
	
}
.layui-col-md10 .label{
	 border-top-left-radius: 0;
	 border-top-right-radius: 10px;
}
.main-form .layui-col-md2{
	border-right:1px solid #ddd;
	height:100%;
	border-top-left-radius: 8px;
    border-bottom-left-radius: 5px;
}
.main-form .layui-col-md4{
	border-right:1px solid #ddd;
	height:100%;
}
.main-form .layui-col-md8{
	height:100%;
}


.main-form .grade tr th{
	padding:5px 0;
	/* border:1px solid #c5dbec; */	
	background: #E7F4F5;
}
.main-form .grade #Role tr td{
	background:#fff;
	height:28px;
	line-height:28px;
	border-right:none;
	font-weight:normal;

}
.main-form .grade #Role tr td span.checkbox{
	float:none;
}
.main-form .grade #Role tr:hover td{
	background:rgb(223, 239, 252);
	color:#e17009;
	/* border-right:1px solid rgb(223, 239, 252); */
	transition:all 0.3s ease;
}

.toolbar{
	width:100%;
	height:40px;
	border-bottom:1px solid #c5dbec;
	line-height:40px;
}
.toolbar a{
	text-align:center;
	display:inline-block;
	width:60px;
	line-height:30px;
	/* background:rgb(223, 239, 252); */
	border:1px solid #ddd;
	border-radius:10px;
	margin-left:10px;
	
}
.toolbar .toolbar_save{
	color:#fff;
	border:none;
	background:#00BBF3;
}
.toolbar a:hover{
	color:#e17009;
}
.toolbar .search{
	display:inline-block;
	margin-left:20px;
	
}
.toolbar .search span{
	color:#666;
	font-size:14px;
}
.toolbar .search .username{
	width: 211px;
    height: 30px;
    line-height: 30px;
    border: 1px solid #ddd;
    color: #333;
    background: #fff;
    margin-right: 5px;
    border-radius: 3px;
    box-shadow: rgba(160, 160, 160, 0.1) 1px 1px 3px;
}
.toolbar .search .toolbar_search{
	margin-left: 10px;
    width: 75px;
    height: 32px;
    line-height: 32px;
    background: #169bd5;
    text-align: center;
    border-radius: 3px;
    color:#fff;
    
}
#module tr td{
	background:#fff;
	height:28px;
	line-height:28px;
	font-weight:normal;
	border-right:1px solid #fff;
}
#module tr:hover td{
	background:rgb(223, 239, 252);
	color:#e17009;
	/* border-right:1px solid rgb(223, 239, 252); */
	transition:all 0.3s ease;
}
#module tr.current td{
	background:#79b7e7;
	color:#e17009;
	/* border-right:1px solid #79b7e7; */
}
.main-form .layui-col-md10
{
