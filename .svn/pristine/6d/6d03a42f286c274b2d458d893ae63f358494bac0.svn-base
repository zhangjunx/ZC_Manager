<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>会议室管理</title>
    <link rel="stylesheet" href="../css/common.css" />
    <link rel="stylesheet" href="../css/public1.css" />
    <link rel="stylesheet" href="../css/list_style.css" />
    <link rel="stylesheet" href="../layui/css/layui.css" media="all">
    <link rel="stylesheet" href="../zjx/accessControl.css" />
    <link rel="stylesheet" href="css/meetingManagement.css" media="all">
	<link rel="stylesheet" href="../css/Shuttle.css">
</head>

<body>
    <div id="cme-main">
        <div class="main-content">
            <div class="main-tab">
                <div class="label fl curr"><a href="javascript:;">会议记录</a></div>
            <div class="main-table">
				<div class="search-box main-form fl">
					<div class="form-term fl clearfix">
						<div class="lable">
							<span></span>会议主题：
						</div>
						<div class="control clearfix">
							<input type="text" id="qmettingtitle" placeholder="输入会议主题" class="input">
						</div>
					</div>
					
					<div class="form-term fl clearfix">
						<div class="lable">
							<span></span>召集人：
						</div>
						<div class="control clearfix">
							<input type="text" id="qpromotername" placeholder="输入会议室名称" class="input">
						</div>
					</div>
					
					<div class="form-term fl clearfix">
                        <div class="lable"><span></span>会议日期：</div>
                        <div class="control clearfix">
                          <input type="text" readonly="readonly" class="input" placeholder="选择会议日期" id="qmeetingdate">
                        </div>
                    </div>
					<div class="btn fl">
						<a href="javascript:;" id="queryDeptListByConditionBtn">搜索</a>
					</div>
				</div>
				<table class="grade personnel" id="dayindaju1">
					<tr>
						<td width="40">编号</td>
						<td width="40">会议类型</td>
						<td width="40">会议主题</td>
						<td width="80">召集人</td>
						<td width="80">主持人</td>
						<td width="80">会议室名称</td>
						<td width="80">会议室位置</td>
						<td width="80">会议内容</td>
						<td width="80">会议日期</td>
						<td width="80">会议时间</td>
						<td width="80">应到人数</td>
						<td width="80">实到人数</td>
						<td width="80">操作</td>
					</tr>
					<tbody id="cont">
					
					</tbody>
				</table>
				<div class="pages" id="pages" id="test">
				</div>

			</div>
		</div>
        </div>
    </div>
    
    <!--会议室选择弹窗  -->
	<div class="vehicleInfo_shadow" id="roomWin"></div>
    <div class="vehicleInfo" id="roomWin1" style="width: 55%;height: 55%;left: 20%;top: 20%;">
		<div class="layer_top">
   			<h2 class="title">查看参会人员</h2>
   			<img src="img/cha.png" class="quxiao" id="roomquxiao">
   		</div>
		<div class="vehicle_top" style="padding-left:10px;padding-right:10px">
			<div class="form-term clearfix" id="personList" style='overflow: auto;text-align: center;'>
	        </div>
		</div>
	</div>
    <script type="text/javascript" src="../hyf/path_js.js"></script>
    <script type="text/javascript" src="../js/jquery.min.js"></script>
    <script type="text/javascript" src="../js/layer/layer.js"></script>
    <script type="text/javascript" src="../layui/layui.all.js"></script>
    <script type="text/javascript" src="js/meetingRecordList.js"></script>
</body>

</html>