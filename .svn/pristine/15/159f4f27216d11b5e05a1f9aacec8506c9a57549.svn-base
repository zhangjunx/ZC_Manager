#shuttle_box{
	zoom: 1;
    box-sizing: border-box;
}
#shuttle_box:after{
   content: ".";
   clear: both;
   display: block;
   height: 0;
   overflow: hidden;
   visibility: hidden;
}
.shuttle_box_li{
    height: 640px;
    float: left;
    
}
.shuttle_box_near .title{
    background: #F5F7FA;
    padding-left: 10px;
    height: 42px;
    line-height: 42px;
    position: relative;
}
.shuttle_box_near .search{
    position: relative;
    width: 100%;
}
.shuttle_box_near .search img{
    width: 18px;
    height: 18px;
    position: absolute;
    top: 18px;
    left: 16px;
}
.shuttle_box_near .search input{
    width: 80%;
    height: 30px;
    margin: 0 auto;
    line-height: 30px;
    border-radius: 5px;
    border: 1px solid #ccc;
    padding-left:30px;
    margin-left: 7px;
    margin-top: 10px;
    margin-bottom: 15px;
}
.shuttle_box_near{
