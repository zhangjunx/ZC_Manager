/* 鐝鍒嗛厤寮瑰嚭灞� */
.shiftInfo li{
	padding: 8px;
    cursor: pointer;
    position:relative;
}
.shiftInfo li span.checkbox{
	font-size: 12px;
    color: #555;
    display: inline-block;
    height: 30px;
    line-height: 30px;
    margin-right: 8px;
    padding-left: 16px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    background: url(../images/icons/form_check.png) no-repeat left 6px;
    position: absolute;
    top: 0px;
    right: 0px;
}
.shiftInfo li.curr span.checkbox{
	background: url(../images/icons/form_check_on.png) no-repeat left 6px;
}
